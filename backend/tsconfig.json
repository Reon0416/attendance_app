{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": ".",
    "paths": {
      "@prisma/client": ["./generated/prisma"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src", "generated", "prisma/generated"]
}